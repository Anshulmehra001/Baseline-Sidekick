{
  "name": "baseline-sidekick",
  "displayName": "Baseline Sidekick",
  "description": "AI-powered baseline compatibility assistant for VS Code - Google Baseline Tooling Hackathon 2025",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.74.0"
  },
  "icon": "icon.png",
  "galleryBanner": {
    "color": "#007ACC",
    "theme": "dark"
  },
  "categories": [
    "Linters",
    "Other",
    "Machine Learning",
    "Programming Languages"
  ],
  "keywords": [
    "baseline",
    "web-platform",
    "compatibility",
    "cross-browser",
    "ai",
    "modernization",
    "polyfill",
    "css",
    "javascript",
    "html",
    "hackathon",
    "google"
  ],
  "author": "Anshulmehra001",
  "publisher": "Anshulmehra001",
  "license": "UNLICENSED",
  "private": true,
  "homepage": "https://github.com/Anshulmehra001/baseline-sidekick#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/Anshulmehra001/Baseline-Sidekick.git"
  },
  "bugs": {
    "url": "https://github.com/Anshulmehra001/Baseline-Sidekick/issues"
  },
  "activationEvents": [
    "onLanguage:css",
    "onLanguage:javascript",
    "onLanguage:typescript",
    "onLanguage:html"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "baselineSidekick.showMainMenu",
        "title": "üöÄ Baseline Sidekick - Main Menu",
        "category": "Baseline Sidekick",
        "icon": "$(rocket)"
      },
      {
        "command": "baselineSidekick.showTutorial",
        "title": "üéì Show Tutorial",
        "category": "Baseline Sidekick",
        "icon": "$(mortar-board)"
      },
      {
        "command": "baselineSidekick.showCompatibilityReport",
        "title": "üìä Compatibility Report",
        "category": "Baseline Sidekick",
        "icon": "$(graph)"
      },
      {
        "command": "baseline.auditWorkspace",
        "title": "üîç Check Current File",
        "category": "Baseline Sidekick"
      },
      {
        "command": "baseline.ai.generatePolyfill",
        "title": "ü§ñ Generate AI Polyfill",
        "category": "Baseline AI"
      },
      {
        "command": "baseline.ai.generateConfig",
        "title": "ü§ñ Generate Build Configuration",
        "category": "Baseline AI"
      },
      {
        "command": "baseline.ai.refactorCode",
        "title": "ü§ñ AI Code Refactoring",
        "category": "Baseline AI"
      },
      {
        "command": "baseline.ai.showAlternatives",
        "title": "ü§ñ Show Baseline Alternatives",
        "category": "Baseline AI"
      },
      {
        "command": "baseline.ai.modernizeFile",
        "title": "ü§ñ Modernize File with AI",
        "category": "Baseline AI"
      },
      {
        "command": "baseline.ai.generateStrategy",
        "title": "ü§ñ Generate Modernization Strategy",
        "category": "Baseline AI"
      },
      {
        "command": "baseline.ai.analyzePerformance",
        "title": "ü§ñ Analyze Performance Impact",
        "category": "Baseline AI"
      },
      {
        "command": "baseline.showScoreDetails",
        "title": "üìä Show Baseline Score Dashboard",
        "category": "Baseline"
      },
      {
        "command": "baseline.ai.modernizationWizard",
        "title": "üßô‚Äç‚ôÇÔ∏è Launch Modernization Wizard",
        "category": "Baseline AI"
      }
    ],
    "keybindings": [
      {
        "command": "baselineSidekick.showMainMenu",
        "key": "ctrl+shift+b",
        "mac": "cmd+shift+b",
        "when": "editorTextFocus"
      },
      {
        "command": "baseline.auditWorkspace",
        "key": "ctrl+alt+b",
        "mac": "cmd+alt+b",
        "when": "editorTextFocus"
      }
    ],
    "configuration": {
      "title": "Baseline Sidekick",
      "properties": {
        "baselineSidekick.performance.debounceDelay": {
          "type": "number",
          "default": 300,
          "minimum": 50,
          "maximum": 2000,
          "description": "Debounce delay for diagnostic updates in milliseconds"
        },
        "baselineSidekick.performance.maxFileSize": {
          "type": "number",
          "default": 5242880,
          "minimum": 1024,
          "description": "Maximum file size to analyze in bytes (default: 5MB)"
        },
        "baselineSidekick.performance.maxCacheSize": {
          "type": "number",
          "default": 10000,
          "minimum": 100,
          "maximum": 100000,
          "description": "Maximum number of items to cache for performance"
        },
        "baselineSidekick.performance.parseTimeout": {
          "type": "number",
          "default": 5000,
          "minimum": 1000,
          "maximum": 30000,
          "description": "Timeout for parsing operations in milliseconds"
        },
        "baselineSidekick.performance.maxAnalysisDepth": {
          "type": "number",
          "default": 50,
          "minimum": 10,
          "maximum": 200,
          "description": "Maximum analysis depth for nested structures"
        },
        "baselineSidekick.performance.enableAsyncProcessing": {
          "type": "boolean",
          "default": true,
          "description": "Enable asynchronous processing for large files"
        },
        "baselineSidekick.performance.largeFileThreshold": {
          "type": "number",
          "default": 102400,
          "minimum": 1024,
          "description": "Threshold for considering a file 'large' in bytes (default: 100KB)"
        },
        "baselineSidekick.ai.geminiApiKey": {
          "type": "string",
          "default": "",
          "description": "Google Gemini API key for AI-powered features (get at https://aistudio.google.com/app/apikey)"
        },
        "baselineSidekick.ai.enableAIFeatures": {
          "type": "boolean",
          "default": true,
          "description": "Enable AI-powered modernization features"
        },
        "baselineSidekick.gamification.enableScoring": {
          "type": "boolean",
          "default": true,
          "description": "Enable baseline compatibility scoring and achievements"
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "test": "vitest --run",
    "test:watch": "vitest"
  },
  "devDependencies": {
    "@types/babel__parser": "^7.0.0",
    "@types/babel__traverse": "^7.28.0",
    "@types/node": "^18.0.0",
    "@types/parse5": "^6.0.3",
    "@types/vscode": "^1.74.0",
    "typescript": "^4.9.4",
    "vitest": "^1.0.0"
  },
  "dependencies": {
    "@babel/parser": "^7.28.4",
    "@babel/traverse": "^7.28.4",
    "parse5": "^8.0.0",
    "postcss": "^8.5.6",
    "web-features": "^3.0.0"
  }
}
